var vid = new Video($('.localVideo')[0]);//,$('.remoteVideo')[0]);
var vidr=null;
var main = function() {

	vid.initLocal();
	
	$('.getRemote').click(function() {
		console.log('getRemove click');
		vidr = new Video($('.remoteVideo')[0]);
		vidr.getCallInitiatorVideo();
	});
}

$(document).ready(main);