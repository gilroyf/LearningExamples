var restServiceChannel = {
		url: "rest/intercomserver";
		sendOffer: function(desc, group) {
			$.ajax({
				  url:this.url;//"rest/intercomserver",
				  type:"POST",
				  data:JSON.stringify( {"type":"initiateCall", "group":"group1", "sdp":desc}),
				  contentType:"application/json",
				  dataType:"json",
				  success: function(){
					  console.log("done");
				  }, 
				  error: function(a,b,c) {
					  console.log("error"+b+" "+c );
				  }
				});
		}
		
}