package com.gilroyfernandes.fileReceiver;

import java.io.*;
import java.net.*;


public class FilReceiver {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Socket s = null;
		try {
			s = new Socket("127.0.0.1", 40000);
		} catch (UnknownHostException e) {
			System.error.println("UnknownHost Exception ", e.getMessage());
		} catch (IOException e) {
			System.error.println("IOException ", e.getMessage());	
		} finally {
			s.close();
		}
	}

}
